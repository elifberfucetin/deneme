<script type="text/javascript">
 google.charts.load("current", {packages:['corechart']});
 google.charts.setOnLoadCallback(drawChart);
 function drawChart() {
 var data = google.visualization.arrayToDataTable([
 ["Element", "Tutar", { role: "style" } ],
 ["İngilizce", <?php echo"$ingilizcetutar"?>, "#b87333"],
 ["Almanca", <?php echo"$almancatutar"?>, "silver"],
 ["Japonca", <?php echo"$japoncatutar"?>, "gold"],
 ["Fransızca", <?php echo"$fransızcatutar"?>, "color: #e5e4e2"],
["Kantin", 4000, "color: green"]
 ]);
 var view = new google.visualization.DataView(data);
 view.setColumns([0, 1,
 { calc: "stringify",
 sourceColumn: 1,
 type: "string",
 role: "annotation" },
 2]);
 var options = {
 title: "Aylık Gelirler Grafiği",
 width: 370,
 height: 370,
 bar: {groupWidth: "95%"},
 legend: { position: "none" },
 };
 var chart = new
google.visualization.ColumnChart(document.getElementById("columnchart_va
lues"));
 chart.draw(view, options);
 }
 </script>
<script type="text/javascript">
 google.charts.load("current", {packages:["corechart"]});
 google.charts.setOnLoadCallback(drawChart);
 function drawChart() {
 var data = google.visualization.arrayToDataTable([
 ['Task', 'Hours per Day'],
 ['Öğretmen Maaşları', 20000],
 ['Bina Kirası', 8000],
 ['Kantin', 3500],
 ['Faturalar', 3000]

 ]);
 var options = {
 title: 'Aylık Giderler Grafiği',
 is3D: true,
 };
 var chart = new
google.visualization.PieChart(document.getElementById('piechart_3d'));
 chart.draw(data, options);
 }
 </script>
<script type="text/javascript">
 google.charts.load('current', {'packages':['corechart']});
 google.charts.setOnLoadCallback(drawVisualization);
 function drawVisualization() {
 // Some raw data (not necessarily accurate)
 var data = google.visualization.arrayToDataTable([
 ['Month', 'Gelirler', 'Giderler','Kar Zarar'],
 ['Mayıs', <?php echo"$mayistutar+4000"; ?>, 34500,<?php
echo"$mayistutar+4000-34500"; ?>],
 ['Haziran', <?php echo"$hazirantutar+4000"; ?>, 34500,<?php
echo"$hazirantutar+4000-34500"; ?>],
 ['Temmuz', <?php echo"$temmuztutar+4000"; ?>, 34500,<?php
echo"$temmuztutar+4000-34500"; ?>],
 ['Ağustos', <?php echo"$agustostutar+4000"; ?>, 34500,<?php
echo"$agustostutar+4000-34500"; ?>],
 ['Eylül', <?php echo"$eylultutar+4000"; ?>, 34500,<?php
echo"$eylultutar+4000-34500"; ?>]
 ]);
 var options = {
 title : 'Aylık Kâr Zarar Grafiği',
 vAxis: {title: 'Tutar'},
 hAxis: {title: 'Aylar'},
 seriesType: 'bars',
 series: {2: {type: 'line'}} };
 var chart = new
google.visualization.ComboChart(document.getElementById('chart_div'));
 chart.draw(data, options);
 }
 </script>
